using System;

namespace FurstStepsInProfessional_22_04
{
    class Program
    {
        static void Main(string[] args)
        {
            double dogPrices = double.Parse(Console.ReadLine());
            double catPrices = double.Parse(Console.ReadLine());
            double cat = catPrices * 4.0;
            double dog = dogPrices * 2.50;
            double sum = cat + dog;
            Console.WriteLine($"{sum} lv.");
        }
    }
}
