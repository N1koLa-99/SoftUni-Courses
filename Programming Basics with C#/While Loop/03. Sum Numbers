using System;

class Program
{
    static void Main()
    {
       
        int targetSum = int.Parse(Console.ReadLine());

        int sum = 0;
        int count = 0;

        while (sum < targetSum)
        {
            
            int number = int.Parse(Console.ReadLine());

            sum += number;
            count++;
        }

        Console.WriteLine(sum);
        Console.ReadLine();
    }
}
