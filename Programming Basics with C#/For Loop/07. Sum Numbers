using System;
namespace For_Loop___Lab
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            int sum = 0;
            for (int i = 0; i <n; i++)
            {
            int nextNum= int.Parse(Console.ReadLine());
                sum = sum + nextNum;
            }
            Console.WriteLine(sum);
        }
    }
}
